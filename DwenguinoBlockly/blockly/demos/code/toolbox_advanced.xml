    <category id="catDwenguino" colour="315" name="DWENGUINO">
        <block type="setup_loop_structure"></block>
        <block type="set_leds">
            <value name="register value">
                <block type="char_type" id="18">
                    <field name="BITMASK">0b10101010</field>
                </block>
            </value>
        </block>
        <block type="dwenguino_constants"></block>
        <block type="char_type"></block>
        <block type="dc_motor">
            <value name="channel">
              <block type="motor_1_2">
                  <field name="NUM">1</field>
              </block>
            </value>
            <value name="speed">
                <block type="math_number" id="37">
                    <field name="NUM">255</field>
                </block>
            </value>
        </block>
        <block type="dwenguino_lcd">
            <value name="text">
                <block type="text" id="48">
                    <field name="TEXT">Hello Dwenguino</field>
                </block>
            </value>
            <value name="line_number">
              <block type="motor_1_2">
                  <field name="NUM">1</field>
              </block>
            </value>
            <value name="character_number">
                <block type="math_number" id="58">
                    <field name="NUM">1</field>
                </block>
            </value>
        </block>
        <block type="clear_lcd"></block>
        <block type="sonar_sensor">
          <value name="trig">
              <block type="base_pins_list">
                  <field name="PIN">11</field>
              </block>
          </value>
          <value name="echo">
              <block type="base_pins_list">
                  <field name="PIN">12</field>
              </block>
          </value>
        </block>
        <block type="remote_sensor">
            <value name="tsop">
                <block type="base_pins_list">
                    <field name="PIN">15</field>
                </block>
            </value>
        </block>
        <block type="dwengo_sensorboard">
            <value name="led">
              <block type="led_LD1_LD2">
                  <field name="PIN">LD1</field>
              </block>
            </value>
            <value name="state">
              <block type="inout_highlow">
                  <field name="BOOL">LOW</field>
              </block>
            </value>
        </block>
        <block type="dwengo_sensorboard_sensor">
            <value name="sensor">
              <block type="sensor_OS">
                  <field name="PIN">OS1</field>
              </block>
            </value>
            <value name="mode">
              <block type="mode_sensor">
                  <field name="PIN">DIFF_MODE</field>
              </block>
            </value>
        </block>
        <block type="dwenguino_servo">
            <value name="channel">
              <block type="base_pins_list">
                  <field name="PIN">1</field>
              </block>
            </value>
            <value name="angle">
                <block type="math_number" id="116">
                    <field name="NUM">45</field>
                </block>
            </value>
        </block>
        <block type="inout_tone_pin">
            <value name="PIN">
                <block type="base_pins_list">
                    <field name="PIN">BUZZER</field>
                </block>
            </value>
            <value name="NUM">
                <block type="char_type">
                    <field name="BITMASK">440</field>
                </block>
            </value>
        </block>
        <block type="inout_notone_pin">
            <value name="PIN">
                <block type="base_pins_list">
                    <field name="PIN">BUZZER</field>
                </block>
            </value>
        </block>
        <block type="inout_buildin_led">
            <value name="STATE">
                <block type="inout_highlow">
                    <field name="BOOL">LOW</field>
                </block>
            </value>
        </block>
